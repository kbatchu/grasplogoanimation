  "version": "0.2.0",

  "configurations": [
    
    {
      "type": "chrome",
      "request": "attach",
      "name": "Attach to Chrome",
      "port": 9222,
      "sourceMaps": true,
      "webRoot": "${workspaceRoot}"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "GraspIntro_Flythrough",
      "url": "http://localhost/grasplogoanimationapp/graspintro_flythrough.html",
      "webRoot": "${workspaceRoot}"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "GraspIntro",
      "url": "http://localhost/grasplogoanimationapp/graspintro.html",
      "webRoot": "${workspaceRoot}"
    }
  ]
}
